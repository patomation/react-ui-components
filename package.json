    "lint": "eslint --ext .js,.ts,.tsx .",
    "dev": "yarn watch:src",
    "test": "jest",
    "coverage": "jest --coverage",
    "demo": "webpack-dev-server --mode development --open --hot",
    "build:demo": "npm run doc && webpack",
    "demo:build": "http-server ./dist",
    "doc": "react-docgen ./src -o ./demo/doc.json --pretty --exclude ['style.js','*.test.js','config.js']",
    "doc2": "./node_modules/.bin/typedoc"
  },
  "husky": {
    "hooks": {
