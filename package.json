{
  "name": "react-ui-components",
  "version": "0.1.0",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "babel src/components --out-dir lib --ignore \"**/*.test.js\"",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "watch:components": "nodemon --watch src/components/ --ignore index.js --ignore [*.js] node utilities/build.components.index.js"
  },
  "dependencies": {
    "formik": "^1.5.8",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "nodemon": "^1.19.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
